<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.circularreveal.CircularRevealRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    tools:context=".BrowserActivity">

    <com.google.android.material.circularreveal.CircularRevealRelativeLayout
        android:id="@+id/toolbarhome"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <include
            layout="@layout/v5_home_toolbar"
            android:layout_width="match_parent"
            android:layout_height="57dp" />

    </com.google.android.material.circularreveal.CircularRevealRelativeLayout>

    <com.google.android.material.circularreveal.CircularRevealRelativeLayout
        android:id="@+id/main_app"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/toolbarhome"
        android:layout_marginTop="1dp"
        android:background="@color/colorPrimary">

        <com.google.android.material.circularreveal.CircularRevealRelativeLayout
            android:id="@+id/notify_screen"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp">

            <com.google.android.material.circularreveal.CircularRevealRelativeLayout
                android:id="@+id/notify_main"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="8dp"
                tools:visibility="gone"
                tools:ignore="UselessParent">

                <include
                    layout="@layout/v5_notify_main"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />

            </com.google.android.material.circularreveal.CircularRevealRelativeLayout>

        </com.google.android.material.circularreveal.CircularRevealRelativeLayout>

        <androidx.core.widget.NestedScrollView
            android:id="@+id/menulist"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/notify_screen"
            android:scrollbars="vertical">

            <com.google.android.material.circularreveal.CircularRevealRelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/facebook"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/facebook_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_facebook"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/messenger"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/facebook"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/messenger_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_messenger"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/instagram"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/messenger"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/instagram_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_instagram"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/twitter"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/instagram"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/twitter_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_twitter"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/tumblr"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/twitter"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/tumblr_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_tumblr"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/pinterest"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/tumblr"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/pinterest_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_pinterest"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/linkedin"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/pinterest"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/linkedin_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_linkedin"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/workplace"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/linkedin"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/workplace_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_workplace"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/myspace"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/workplace"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/myspace_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_myspace"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/flickr"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/myspace"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/flickr_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_flickr"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/periscope"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/flickr"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/periscope_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_periscope"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/meetup"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/periscope"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/meetup_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_meetup"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/weibo"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/meetup"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/weibo_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_weibo"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/badoo"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/weibo"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/badoo_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_badoo"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/twoo"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/badoo"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/twoo_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_twoo"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/quora"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/twoo"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/quora_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_quora"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/wikipedia"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/quora"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/wikipedia_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_wiki"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/wattpad"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/wikipedia"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/wattpad_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_wattpad"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/reddit"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/wattpad"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/reddit_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_reddit"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/medium"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/reddit"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/medium_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_medium"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/askfm"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/medium"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/askfm_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_askfm"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/livejournal"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/askfm"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/livejournal_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_livejournal"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/soundcloud"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/livejournal"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/soundcloud_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_soundcloud"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/lastfm"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/soundcloud"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/lastfm_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_lastfm"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/vk"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/lastfm"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/vk_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_vk"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/twitch"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/vk"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/twitch_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_twitch"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/dailymotion"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/twitch"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/dailymotion_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_dailymotion"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/youtube"
                    android:layout_width="match_parent"
                    android:layout_height="52dp"
                    android:layout_below="@id/dailymotion"
                    android:layout_marginBottom="4dp"
                    android:background="@android:color/transparent"
                    android:drawableStart="@drawable/youtube_ico"
                    android:drawablePadding="12dp"
                    android:paddingStart="12dp"
                    android:text="@string/app_name_youtube"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textSize="16sp"
                    tools:ignore="RtlSymmetry" />

                <com.google.android.material.circularreveal.CircularRevealRelativeLayout
                    android:id="@+id/menubottom"
                    android:layout_width="match_parent"
                    android:layout_height="32dp"
                    android:layout_below="@id/youtube"
                    android:layout_marginBottom="8dp">

                    <include
                        layout="@layout/v5_privacyterms_buttons"
                        android:layout_width="match_parent"
                        android:layout_height="32dp" />

                </com.google.android.material.circularreveal.CircularRevealRelativeLayout>

            </com.google.android.material.circularreveal.CircularRevealRelativeLayout>

        </androidx.core.widget.NestedScrollView>

    </com.google.android.material.circularreveal.CircularRevealRelativeLayout>

</com.google.android.material.circularreveal.CircularRevealRelativeLayout>
