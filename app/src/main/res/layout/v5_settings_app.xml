<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.circularreveal.CircularRevealRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/info"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:background="?selectableItemBackground"
        android:clickable="true"
        android:focusable="true"
        android:paddingStart="60dp"
        android:paddingTop="12dp"
        android:paddingEnd="20dp"
        android:paddingBottom="12dp"
        android:text="@string/info_settings"
        android:textAlignment="textStart"
        android:textAllCaps="false"
        android:textColor="@color/settings_info_color"
        android:textSize="15sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="44dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/categoryTheme"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/info"
        android:layout_marginStart="60dp"
        android:layout_marginTop="18dp"
        android:layout_marginBottom="10dp"
        android:clickable="true"
        android:focusable="true"
        android:text="@string/categoryTheme"
        android:textColor="@color/colorAccent"
        android:textSize="15sp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/theme_btn"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:layout_below="@+id/categoryTheme"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="6dp"
        android:background="@drawable/ripple"
        android:clickable="true"
        android:focusable="true"
        android:paddingStart="60dp"
        android:paddingTop="16dp"
        android:paddingEnd="60dp"
        android:text="@string/setting_sw15"
        android:textAlignment="textStart"
        android:textAllCaps="false"
        android:textColor="@color/settings_text_color"
        android:textSize="16sp"
        tools:ignore="RtlSymmetry"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="44dp" />

    <androidx.appcompat.widget.SwitchCompat
        android:id="@+id/themesetting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/theme_btn"
        android:layout_alignEnd="@+id/theme_btn"
        android:layout_marginTop="-40dp"
        android:checked="false"
        android:paddingEnd="20dp"
        android:visibility="visible"
        tools:ignore="RtlSymmetry" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/categoryBrowser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/theme_btn"
        android:layout_marginStart="60dp"
        android:layout_marginTop="18dp"
        android:layout_marginBottom="10dp"
        android:clickable="true"
        android:focusable="true"
        android:text="@string/categoryBrowser"
        android:textColor="@color/colorAccent"
        android:textSize="15sp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/javascript_btn"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:layout_below="@+id/categoryBrowser"
        android:layout_alignStart="@+id/theme_btn"
        android:layout_marginTop="6dp"
        android:background="@drawable/ripple"
        android:clickable="true"
        android:focusable="true"
        android:paddingStart="60dp"
        android:paddingTop="16dp"
        android:paddingEnd="60dp"
        android:text="@string/setting_sw1"
        android:textAlignment="textStart"
        android:textAllCaps="false"
        android:textColor="@color/settings_text_color"
        android:textSize="16sp"
        android:visibility="visible"
        tools:ignore="RtlSymmetry"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="44dp" />

    <androidx.appcompat.widget.SwitchCompat
        android:id="@+id/javascript"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/javascript_btn"
        android:layout_alignEnd="@+id/javascript_btn"
        android:layout_marginTop="-40dp"
        android:paddingEnd="20dp"
        android:checked="false"
        android:visibility="visible"
        tools:ignore="RtlSymmetry" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/categoryAccessibility"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/javascript_btn"
        android:layout_marginStart="60dp"
        android:layout_marginTop="18dp"
        android:layout_marginBottom="10dp"
        android:clickable="true"
        android:focusable="true"
        android:text="@string/categoryAccessibility"
        android:textColor="@color/colorAccent"
        android:textSize="15sp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/minimode_btn"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:layout_below="@id/categoryAccessibility"
        android:layout_alignStart="@+id/theme_btn"
        android:layout_marginTop="6dp"
        android:background="@drawable/ripple"
        android:clickable="true"
        android:focusable="true"
        android:paddingStart="60dp"
        android:paddingTop="16dp"
        android:paddingEnd="60dp"
        android:text="@string/setting_sw11"
        android:textAlignment="textStart"
        android:textAllCaps="false"
        android:textColor="@color/settings_text_color"
        android:textSize="16sp"
        android:visibility="visible"
        tools:ignore="RtlSymmetry"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="44dp" />

    <androidx.appcompat.widget.SwitchCompat
        android:id="@+id/minimode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/minimode_btn"
        android:layout_alignEnd="@+id/minimode_btn"
        android:layout_marginTop="-40dp"
        android:paddingEnd="20dp"
        android:checked="false"
        android:visibility="visible"
        tools:ignore="RtlSymmetry" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/desktopmode_btn"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:layout_below="@+id/minimode_btn"
        android:layout_alignStart="@+id/theme_btn"
        android:layout_marginTop="6dp"
        android:background="@drawable/ripple"
        android:clickable="true"
        android:focusable="true"
        android:paddingStart="60dp"
        android:paddingTop="16dp"
        android:paddingEnd="60dp"
        android:text="@string/setting_sw6"
        android:textAlignment="textStart"
        android:textAllCaps="false"
        android:textColor="@color/settings_text_color"
        android:textSize="16sp"
        android:visibility="visible"
        tools:ignore="RtlSymmetry"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="44dp" />

    <androidx.appcompat.widget.SwitchCompat
        android:id="@+id/desktopmode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/desktopmode_btn"
        android:layout_alignEnd="@+id/desktopmode_btn"
        android:layout_marginTop="-40dp"
        android:paddingEnd="20dp"
        android:checked="false"
        android:visibility="visible"
        tools:ignore="RtlSymmetry" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/categoryMera"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/desktopmode_btn"
        android:layout_marginStart="60dp"
        android:layout_marginTop="18dp"
        android:layout_marginBottom="10dp"
        android:clickable="true"
        android:focusable="true"
        android:text="@string/app_name"
        android:textColor="@color/colorAccent"
        android:textSize="15sp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/clearcache"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:layout_below="@+id/categoryMera"
        android:layout_marginTop="6dp"
        android:background="@drawable/ripple"
        android:clickable="true"
        android:focusable="true"
        android:paddingStart="60dp"
        android:paddingTop="16dp"
        android:text="@string/clear_files"
        android:textAlignment="textStart"
        android:textAllCaps="false"
        android:textColor="@color/settings_text_color"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="44dp"
        tools:ignore="RtlSymmetry" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/restart"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:layout_below="@+id/clearcache"
        android:layout_alignStart="@+id/theme_btn"
        android:layout_marginTop="6dp"
        android:background="@drawable/ripple"
        android:paddingStart="60dp"
        android:paddingTop="16dp"
        android:clickable="true"
        android:focusable="true"
        android:text="@string/restart_button"
        android:textAlignment="textStart"
        android:textAllCaps="false"
        android:textColor="@color/settings_text_color"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="44dp"
        tools:ignore="RtlSymmetry" />

</com.google.android.material.circularreveal.CircularRevealRelativeLayout>
